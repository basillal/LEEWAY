{
    "version": 2,
    "builds": [
      {
        "src": "dist/core/**",
        "use": "@vercel/static"
      },
      {
        "src": "dist/shell/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      { "src": "/remoteEntry.js", "dest": "/remoteEntry.js" },
      { "src": "/assets/(.*)", "dest": "/assets/$1" },
      { "src": "/(.*)", "dest": "/index.html" }
    ]
  }
  